import pandas as pd MultiLayered Perceptron(9)
dataset = pd.read_csv(r"/Users/shreygupta/Dataset/wine.csv")
x = dataset.iloc[:,0:13]
y = dataset.iloc[:,13]
from sklearn.model_selection import train_test_split
xtrain, xtest, ytrain, ytest = train_test_split(x,y,test_size=0.2, random_state=0)
from sklearn.preprocessing import StandardScaler
sc = StandardScaler()
xtrain = sc.fit_transform(xtrain)
xtest = sc.transform(xtest)
from sklearn.neural_network import MLPClassifier
model = MLPClassifier(hidden_layer_sizes=(10,3,10), max_iter=10000)
model. fit(xtrain, ytrain)
ypred = model.predict(xtest)
from sklearn.metrics import accuracy_score, confusion_matrix
print("Confusion: \n", confusion_matrix(ytest, ypred))
print(â€œAccuracy: ", accuracy_score(ytest, ypred))
